{
  "expo": {
    "name": "SceneSnitch",
    "slug": "scenesnitch-clean",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SceneSnitch.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/SceneSnitch.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "SceneSnitch uses your location to find filming locations near you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SceneSnitch.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": []
  }
}